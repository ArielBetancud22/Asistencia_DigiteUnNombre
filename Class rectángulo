class Rectangulo:

    def __init__(self, base, altura):
        self.altura = altura
        self.base = base

    def calcular_area(self):
        return self.base * self.altura


base = int(input('Digite la base del rect치ngulo'))
altura = int(input('Digite la altura del rect치ngulo'))
rectangulo1 = Rectangulo(base, altura)
print(f'El 치rea del rect치ngulo es: {rectangulo1.calcular_area()}')
